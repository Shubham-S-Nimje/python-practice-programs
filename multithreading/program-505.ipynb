   "source": [
    "import threading\n",
    "s=0\n",
    "def sum_of_numbers(m,n):\n",
    "    global s \n",
    "    for i in range(m,n):\n",
    "        s=s+i\n",
    "\n",
    "\n",
    "t1=threading.Thread(target=sum_of_numbers,args=(1,100))\n",
    "t1.start()\n",
    "t1.join()\n",
    "print(f'Sum is {s}')"
   ]
  }
